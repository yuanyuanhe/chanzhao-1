.col-md-2{
float:left;
width:50%;
}
.msg{
  padding:30px 0px;
  width:100%;
  
}
.Content{
  padding:0 70px;
}
.Model-head{
  font-size: 20px;
  color: #333333;
  background: #F4F4F4;
    height: 56px;
    line-height: 56px;
    padding-left: 40px;
    border-bottom:1px solid #e5e5e5;
}
.msg ul{
  
  position: relative;
}
.msg ul .avatar {
  width:200px;
  height:200px;
  top:60px;
  right:70px;
  position: absolute;
}
.msg ul .avatar img{
  width:100%;
  height:100%;
}
